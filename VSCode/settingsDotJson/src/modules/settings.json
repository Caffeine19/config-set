{
    "$schema": "vscode://schemas/settings/user",

    // auto hide
    // secondary side bar
    "autoHide.autoHideAuxiliaryBar": false,

    // console ninja 使用免费社区版或其他
    "console-ninja.featureSet": "Community",
    "console-ninja.showWhatsNew": false,

    //dev container
    "dev.containers.executeInWSL": true,

    "diffEditor.ignoreTrimWhitespace": true,

    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning"
        // 隐藏 Code Spell Checker 的提示
        // "info",
    ],

    // 直接复制
    "explorer.confirmPasteNative": false,

    // 文件关联方式
    "files.associations": { },

    // file utils
    "fileutils.inputBox.pathType": "workspace",

    //git 自动拉取
    "git.autofetch": true,
    // git
    "git.openRepositoryInParentFolders": "never",

    /* github copilot start */
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": false,
        "scminput": false,
        "vue": true
    },
    "github.copilot.nextEditSuggestions.enabled": false,

    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4o",

    /* gitlens start */
    "gitlens.views.commitDetails.files.layout": "list",

    // haskell
    "haskell.manageHLS": "GHCup",

    // js显示参数名
    "javascript.inlayHints.parameterNames.enabled": "all",
    // js自动更新应用
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.format.keepLines": true,

    /* powermode start */
    // "powermode.enabled":true,
    "powermode.shake.intensity": 0.5,

    // wsl使用插件的方式
    "remote.extensionKind": {
        "GitHub.copilot": [
            "ui"
        ],
        "github.copilot-chat": "ui"
    },

    /* security start */
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.untrustedFiles": "open",

    // svelte
    "svelte.enable-ts-plugin": true,

    // tabnine
    "tabnine.experimentalAutoImports": true,

    /* turbo console log start */
    // 显示文件名
    "turboConsoleLog.includeFilename": true,
    // 显示行号
    "turboConsoleLog.includeLineNum": true,

    // ts显示参数名
    "typescript.inlayHints.parameterNames.enabled": "all",
    //ts自动更新引用
    "typescript.updateImportsOnFileMove.enabled": "always",

    // custom css and js
    "vscode_custom_css.imports": [
        "file:///Users/caffeinecat/.config/vscode-custom-css-and-js/custom.css",
        "file:///Users/caffeinecat/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
    ],

    /* window start */
    "window.menuBarVisibility": "compact",
    "window.titleBarStyle": "custom"
}