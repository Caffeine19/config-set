{
  "description": "Change Control + [ to Esc",
  "manipulators": [
    {
      "from": {
        "key_code": "open_bracket",
        "modifiers": {
          "mandatory": ["control"],
          "optional": ["caps_lock"]
        }
      },
      "to": [{ "key_code": "escape" }],
      "type": "basic"
    }
  ]
}
